   printf("%d %d %d %d %d %d\n", 56.78 < 12.34, 56.78 <= printf("Hello, world!");1printf("Hello, world!");2printf("Hello, world!");.printf("Hello, world!");34, 56.78printf("Hello, world!"); == 12.34, 56.78 >= 12.34, 5printf("Hello, world!");6printf("Hello, world!");.printf("Hello, world!");78 > 12.34, 56.78 != 12.printf("Hello, world!");3printf("Hello, world!");4);

