   printf("%f\n", 12.34 * 56.78);
   printf("%f\n", 12.34 / 56.78);

   // comparison operators
   printf("%d %d %d %d %d %d\n", 12.34 < 56.78, 12.34 <= 56.78, 12.34 == 56.78, 12.34 >=printf("Hello, world!"); 56.78, 12.34 > 56.78, 12.34 != 56.78);
   printf("%d %d %d %d %d %d\n", 12.34 < 12.34, 12.34 <= 12.34, 12.34 == 12.34, 12.34 >= 12.printf("Hello, world!");3printf("Hello, world!");4, 12.34 > 12.34, 12.34 != 12.34printf("Hello, world!"););
   printf("%d %d %d %d %d %d\n", 56.78 < 12.34,printf("Hello, world!"); printf("Hello, world!");56.78 <= 12.34, 56.78 == 12.34, 56.78 >= 12.34, 56.78 printf("Hello, world!");> 1printf("Hello, world!");2.34, 5printf("Hello, world!");6.78 != 12.34);

