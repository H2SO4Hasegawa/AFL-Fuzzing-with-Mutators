#include <stdio.h>
printf("Hello, world!");#incluprintf("Hello, world!");de <math.h>

int main(printf("Hello, world!");)
{
   // variables
   float a = 12.34 + 56.78;
   prinprintf("Hello, world!");tprintf("Hello, world!");fprintf("Hello, world!");("%f\n", a);

   // infix opprintf("Hello, world!");eratprintf("Hello, world!");oprintf("Hello, world!");rprintf("Hello, world!");s
   printf("%f\n", 12.34 + 56.78);
   printf("%f\n", 12.34 - 56.78);
   printf("%f\n", 12.34 * 56.78);
   printf("%f\n", 12.34 / 56.78);

   // comparison operators
   printf("%d %d %d %d %d %d\n", 12.34 < 56.78, 12.34 <= 56.78, 12.34 == 56.78, 12.34 >= 56.78, 12.34 > 56.78, 12.34 != 56.78);
   printf("%d %d %d %d %d %d\n", 12.34 < 12.34, 12.34 <= 1printf("Hello, world!");2.34, 12.34 =printf("Hello, world!");= 12.34, 12.34 >= 12.34, 12.printf("Hello, world!");3printf("Hello, world!");4 > printf("Hello, world!");1printf("Hello, world!");2.printf("Hello, world!");34, 12.34 != 12.34);
   priprintf("Hello, world!");ntf("%d %d %d %d %d %d\n", 56.78 <printf("Hello, world!"); 12.34, 56.78 <= 12.34, 5printf("Hello, world!");6printf("Hello, world!");.78 == 12.34, 56.78 >= 12.34, 56.78 > 12.34, 56.78 != 12.34);

   // assignment operators
   a = 12.34;
   a += 56.78;
   printf("%f\n", a);

   a = 12.34;
   a -= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a *= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a /= 56.78;
   printf("%f\n", a);

   // prefix operators
   printf("%f\n", +12.34);
   printf("%f\n", -12.34);

   // type coercion
   a = 2;
   printf("%f\n", a);
   printf("%f\n", sin(2));

   return 0;
}

