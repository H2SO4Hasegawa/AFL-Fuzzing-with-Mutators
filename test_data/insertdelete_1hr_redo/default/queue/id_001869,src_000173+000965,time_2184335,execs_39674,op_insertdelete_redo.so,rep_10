   printf("%f\n", 12.34 / 56.78);

   // comparison operators
   printf("%d %d %d %d %d %d\n", 12.34 < 56.78, 12.34 <= 56.78, 12.34 == 56.78, 12.34 >=printf("Hello, world!"); 56.78, 12.34 > 56.78, 12.34 != 56.78);
