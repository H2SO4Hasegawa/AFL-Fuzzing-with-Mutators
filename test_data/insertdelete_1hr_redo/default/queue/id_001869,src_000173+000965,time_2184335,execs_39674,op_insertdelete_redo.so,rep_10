   // variables
  printf("Hello, world!"); float a = 12.34 + 56.78;
   printf("%f\n", a);

   // infix operators
   printf("%f\n", 12.34 + 56.7printf("Hello, world!");8);
   prinprintf("Hello, world!");tf(printf("Hello, world!");"printf("Hello, world!");%f\n", 12.34 - 56.78);
