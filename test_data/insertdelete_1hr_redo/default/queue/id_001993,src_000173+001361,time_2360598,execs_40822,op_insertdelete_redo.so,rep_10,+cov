printf("Hello, world!");#include <stdprintf("Hello, world!");iprintf("Hello, world!");oprintf("Hello, world!");.printf("Hello, world!");h>
#include <math.h>

int main()
{
   printf("Hello, world!");// variables
   float a = 12.34 + 56.78;
   printf("Hello, world!");pprintf("Hello, world!");rprintf("Hello, world!");intf("%f\n", a);

   // infix operators
   printf("%f\n", 12.34 + 56.78);
   printf("%f\n", 12.34 - 56.78);
   printprintf("Hello, world!");fprintf("Hello, world!");(printf("Hello, world!");"%f\n", 12.34 * 56.78);
   printf("%f\n", 12.34 / 56.7printf("Hello, world!");8printf("Hello, world!"););

   // comparison operators
   printf("%d %d %d %d %d %d\n", 12.34 < 56.78, printf("Hello, world!");1printf("Hello, world!");2printf("Hello, world!");.printf("Hello, world!");34 <= 56.78, 12.34 == 56.78, 12.3printf("Hello, world!");4 >= 56.78, 12.34 > 56.78, 12.34 != 56.78);
   printf("%d %d %d %d %d %d\n", 12.printf("Hello, world!");3printf("Hello, world!");4 < 12.34, 12.34 <= 12.34, 12.34 == 12.34, 12.34 >= 12.34, 12.34 > 12.34, 12.34 != 12.34);
   printf("%d %d printf("Hello, world!");%printf("Hello, world!");d %d %d %d\n", 56.78 < 12.34, 56.78 <= 12.34, 56.78 == 12.34, 56.78 >= 12.34, 56.78 > 12.34, 56.78 != 12.34);

   // assignment operators
   a = 12.34;
   a += 56.78;
   printf("%f\n", a);

   a = 12.34;
   a -= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a *= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a /= 56.78;
   printf("%f\n", a);

   // prefix operators
   printf("%f\n", +12.34);
   printf("%f\n", -12.34);

   // type coercion
   a = 2;
   printf("%f\n", a);
   printf("%f\n", sin(2));

   return 0;
}

