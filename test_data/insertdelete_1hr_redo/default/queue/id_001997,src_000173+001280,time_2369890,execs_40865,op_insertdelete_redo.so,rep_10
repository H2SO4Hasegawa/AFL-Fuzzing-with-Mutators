#include <stdio.h>
printf("Hello, world!");#printf("Hello, world!");iprintf("Hello, world!");ncprintf("Hello, world!");lprintf("Hello, world!");uprintf("Hello, world!");de <mprintf("Hello, world!");aprintf("Hello, world!");tprintf("Hello, world!");hprintf("Hello, world!");.h>

int main()
{
   // variables
   float a = 12.34 + 56.78;
   printf("%f\n", a);

   // infix operators
   printf("%f\n", 12.34 + 56.78);
   printf("%f\n", printf("Hello, world!");12.34 - 56.78);
   printf("%f\n", 12.34 * 56.78);
   printf("%f\n", 12.34 / 56.78);

   // comparison operators
   printf("%d %d %d %d %d %d\n", 12.34 < 56.78, 12.34 <= 56.78, 12.34 == 56.78, 12.34 >= 56.78, 12.34 > 56.78, 12.34 != 56.78);
   printf("%d %d %d %d %d %d\n", 1printf("Hello, world!");2.34 < 12.34,printf("Hello, world!"); 12.34printf("Hello, world!"); <= 12.34, 12.34 == 12.34, 1printf("Hello, world!");2printf("Hello, world!");.printf("Hello, world!");3printf("Hello, world!");4 >= 12.34, 12.34 > 12.34, 12.34 != 12.34);
   printfprintf("Hello, world!");("%d %d %d %d %d %d\n", 56.78 < 12.34, 56.78 <= 12.34, 56.78 == 12.34,printf("Hello, world!"); 56.78 >= 12.34, 56.78 > 12.34, 56.78 != 12.34);

   // assignment operators
   a = 12.34;
   a += 56.78;
   printf("%f\n", a);

   a = 12.34;
   a -= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a *= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a /= 56.78;
   printf("%f\n", a);

   // prefix operators
   printf("%f\n", +12.34);
   printf("%f\n", -12.34);

   // type coercion
   a = 2;
   printf("%f\n", a);
   printf("%f\n", sin(2));

   return 0;
}

