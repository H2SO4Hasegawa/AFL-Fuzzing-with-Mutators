printf("Hello, world!");#printf("Hello, world!");include <math.h>

int main()
{
   // variables
   float a printf("Hello, world!");= 12.34 + printf("Hello, world!");5printf("Hello, world!");6.78;
   printf("%f\printf("Hello, world!");nprintf("Hello, world!");"printf("Hello, world!");,printf("Hello, world!"); a);

   // infix operators
   printf("%f\n", 12.34 + 56printf("Hello, world!");.printf("Hello, world!");78);
   printf("%f\n", 12.34 - 56.78);
   printf("%f\n", 12.34 * 56.78);
   printf("%f\n", 12.34 / 56.78);

   // comparison operators
   printf("%d %d %d %d %d %d\n", 12.34 < 56.78, 12.34 <= 56.78, 12.34 ==printf("Hello, world!"); printf("Hello, world!");5printf("Hello, world!");6printf("Hello, world!");.78, 12.34 >= 56.78, 12.34 > 56.78, 12.34 != 56printf("Hello, world!");.78);
   prinprintf("Hello, world!");tprintf("Hello, world!");f("printf("Hello, world!");%d %d %d %d %d %d\n", 12.34printf("Hello, world!"); printf("Hello, world!");< 12.34, printf("Hello, world!");1printf("Hello, world!");2printf("Hello, world!");.34 <= 12.34, 12.34 == 12.34, 12.34 >= 12.34, 12.34 > 12.34, 12.34 != 12.34);
