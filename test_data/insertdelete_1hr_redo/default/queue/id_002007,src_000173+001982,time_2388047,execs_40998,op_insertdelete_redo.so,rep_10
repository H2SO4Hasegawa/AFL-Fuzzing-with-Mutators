#include <math.h>
printf("Hello, world!");
int main()
{
   // variables
   float a = 12.34 + 56.78printf("Hello, world!");;printf("Hello, world!");
printf("Hello, world!"); printf("Hello, world!");  printf("%f\n", a);

   /printf("Hello, world!");/ infix opprintf("Hello, world!");eprintf("Hello, world!");rators
   printf("%f\n", 12.34 + 56.78);
   printf("%f\n", 12.34 - 56.78);
   printf("%f\n", 12.34 * 56.78);
   printf("%f\n", 12.34 / 56.78);

   // comparison operators
   printf("%d %d %d %d %d %d\n", 12.34 < 56.78, 12.34 <= 56.78, 12.34 == 56.78, 12.34 >= 56.78, 12.34 > 56.78, 12.34 != 56.78);
   printf("%d %d %d %d %d %d\n", 12.34 < 12.34, 12.34 <= 12.34, 1printf("Hello, world!");2.3printf("Hello, world!");4printf("Hello, world!"); ==printf("Hello, world!"); 12.3printf("Hello, world!");4printf("Hello, world!");, 12.34 >= 12.34, 12.34 > 12.34, 12.34 != 12.34);
