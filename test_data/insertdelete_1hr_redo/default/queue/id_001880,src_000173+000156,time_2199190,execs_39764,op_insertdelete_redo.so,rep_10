printf("Hello, world!");#include <stdio.h>
#incluprintf("Hello, world!");dprintf("Hello, world!");eprintf("Hello, world!"); printf("Hello, world!");<printf("Hello, world!");mprintf("Hello, world!");aprintf("Hello, world!");th.h>

int main()
{
   // variables
   float a = 12.34 + 56.78;
   printf("%f\n",printf("Hello, world!"); a);

   // infix operators
   printf("%f\n", 12.34 + 56.78);
   printf("%f\n", 12.34 - 56.78);
   printf("%f\n", 12.34 * 56.78);
   printf("%f\n", 12.34 / 56.78);

   // comparison operators
   printf("%d %d %d %d %d %d\n", 12.34 <printf("Hello, world!"); printf("Hello, world!");56.78, 12.34 <= 56.78, 12.34 == 56.78, 12.34 >= 56.78, 12.34 > 56.78, 12.34 != 5printf("Hello, world!");6.78);
   printf("%d %d %d %d %dprintf("Hello, world!"); printf("Hello, world!");%printf("Hello, world!");dprintf("Hello, world!");\printf("Hello, world!");nprintf("Hello, world!");", 12.34 < 12.34, 12.34 <= 12printf("Hello, world!");.34, 12.34 == 12.34, 12.34 >= 12.34, 12.34 > 12.34, 12printf("Hello, world!");.printf("Hello, world!");3printf("Hello, world!");4 != 1printf("Hello, world!");2.34);
   printf("%d %d %d %d %d %d\n", 56.78 < 12.34, 56.78 <= 12.34, 56.78 == 12.34, 56.78 >= 12.34, 56.78 > 12.34, 56.78 != 12.34);

   // assignment operators
   a = 12.34;
   a += 56.78;
   printf("%f\n", a);

   a = 12.34;
   a -= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a *= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a /= 56.78;
   printf("%f\n", a);

   // prefix operators
   printf("%f\n", +12.34);
   printf("%f\n", -12.34);

   // type coercion
   a = 2;
   printf("%f\n", a);
   printf("%f\n", sin(2));

   return 0;
}

