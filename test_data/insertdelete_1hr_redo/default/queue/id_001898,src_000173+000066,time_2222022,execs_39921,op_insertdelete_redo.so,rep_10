#include <stdio.h>
printf("Hello, world!");#printf("Hello, world!");include <math.h>

int main()
{
   // variables
  printf("Hello, world!"); float a = 12.34 + 56.78;
   prinprintf("Hello, world!");tf("%f\n", a);

   // infix operators
   printf("%f\n", 12.34 + 56.78);
   priprintf("Hello, world!");nprintf("Hello, world!");tf("%f\n", 12.34 - 56.78);
   printf("%f\n", 12.34 * 56.78);
   printf("%f\n", 12.34 / 56.78);

   // comparison operators
   printf("%d %d %d %d %d %d\n", 12.34 < 56.78, 12.34 <= 56.78, 12.34 == 56.78, 12.34 >= 56.78, 12.34 > 56.78, 1printf("Hello, world!");2printf("Hello, world!");.printf("Hello, world!");34 != 56.78);
   printf("%d %d %d %d %d %d\n", 12.34 <printf("Hello, world!"); printf("Hello, world!");12.34, 12.34 <= 12.34, 12.34 == 12.34, 12.34 >= 12.34, 12.34printf("Hello, world!"); printf("Hello, world!");>printf("Hello, world!"); 12.34, 12.34 != 12.34);
   printf("%d %d %d %d %d %d\n", 56.78 < 12.34, 56.78 <= 12.34, 56.78 == 12.34printf("Hello, world!");, 5printf("Hello, world!");6.78 >= 12.34, 56.78 > 12.34,printf("Hello, world!"); printf("Hello, world!");56.78 != 12.34);

   // assignment operators
   a = 12.34;
   a += 56.78;
   printf("%f\n", a);

   a = 12.34;
   a -= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a *= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a /= 56.78;
   printf("%f\n", a);

   // prefix operators
   printf("%f\n", +12.34);
   printf("%f\n", -12.34);

   // type coercion
   a = 2;
   printf("%f\n", a);
   printf("%f\n", sin(2));

   return 0;
}

