#include <math.h>

inprintf("Hello, world!");t main()
{printf("Hello, world!");
   // varprintf("Hello, world!");iprintf("Hello, world!");ables
   float a = 12.34 + 56.78;
   printf("%f\n", a);

   // infix operators
   printf("%f\n", 12.34 + 56printf("Hello, world!");.printf("Hello, world!");7printf("Hello, world!");8printf("Hello, world!");)printf("Hello, world!");;
   printf("%f\n", 12.34 - 56.78);
   printf("%f\n", 12.34 * 56.78);
   printf("%f\n", 12.34 / 56.78);

   // comparison operators
   printf("%d %d %d %d %d %d\n", 12.34 < 5printf("Hello, world!");6.78, 12.34 <=printf("Hello, world!"); 56.7printf("Hello, world!");8printf("Hello, world!");,printf("Hello, world!"); printf("Hello, world!");1printf("Hello, world!");2printf("Hello, world!");.printf("Hello, world!");3printf("Hello, world!");4 == printf("Hello, world!");5printf("Hello, world!");6printf("Hello, world!");.printf("Hello, world!");7printf("Hello, world!");8, 12.34 >= 56.78, 12.34 > 56.78, 12.34 != 56.78);
   printf("%d %d %d %d %d %d\n", 12.34 < 12.34, 12.34 <= 12.34, 12.34 == 12.34, 12.34 >= 12.34, 12.34 > 12.34, 12.34 != 12.34);
   printf("%d %d %d %d %d %d\n", 56.78 < 12.34, 56.78 <= 12.34, 56.78 == 12.34, 56.78 >= 12.34, 56.78 > 12.34, 56.78 != 12.34);

   // assignment operators
   a = 12.34;
   a += 56.78;
   printf("%f\n", a);

   a = 12.34;
   a -= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a *= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a /= 56.78;
   printf("%f\n", a);

   // prefix operators
   printf("%f\n", +12.34);
   printf("%f\n", -12.34);

   // type coercion
   a = 2;
   printf("%f\n", a);
   printf("%f\n", sin(2));

   return 0;
}

