#include <stdio.h>
#include <math.h>
printf("Hello, world!");
int mprintf("Hello, world!");ain(printf("Hello, world!");)
{
   // variables
   float a = 12.34 + 56.78;
   prinprintf("Hello, world!");tf("%f\n", a);

   // infix operators
   priprintf("Hello, world!");nprintf("Hello, world!");tprintf("Hello, world!");fprintf("Hello, world!");(printf("Hello, world!");"printf("Hello, world!");%f\n", 12.34 + 56.78);
   printf("%f\n", printf("Hello, world!");1printf("Hello, world!");2printf("Hello, world!");.printf("Hello, world!");34 - 56.78);
   printf("%f\n"printf("Hello, world!");,printf("Hello, world!"); 12.34 * 56.78);
   printf("%f\n", 12.34 / 56.78);

   // comparison operators
 printf("Hello, world!");  printf("%d %d %printf("Hello, world!");dprintf("Hello, world!"); %d %d %d\n", 12.3printf("Hello, world!");4printf("Hello, world!"); < printf("Hello, world!");5printf("Hello, world!");6.78, 12.34 <= 56.78, 12.3printf("Hello, world!");4 == 56.78, 12.34 >= 56.78, 12.34 > 56.78, 12.34 != 5printf("Hello, world!");6.78);
   printf("%d %d %d %d %d %d\n", 12.34 < 12.34, 12.34 <= 12.34, 1printf("Hello, world!");2.34 == 12.34, 12.34 >= 12.34, 12.34 > 12.34, 12.34 != 12.34);
   printf("%d %d %d %d %d %d\n", 56.78 < 12.34, 56.78 <= 12.34, 56.78 == 12.34, 56.78 >= 12.34, 56.78 > 12.34, 56.78 != 12.34);

   // assignment operators
   a = 12.34;
   a += 56.78;
   printf("%f\n", a);

   a = 12.34;
   a -= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a *= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a /= 56.78;
   printf("%f\n", a);

   // prefix operators
   printf("%f\n", +12.34);
   printf("%f\n", -12.34);

   // type coercion
   a = 2;
   printf("%f\n", a);
   printf("%f\n", sin(2));

   return 0;
}

