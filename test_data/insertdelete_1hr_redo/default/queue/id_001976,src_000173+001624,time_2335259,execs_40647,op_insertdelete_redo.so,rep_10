printf("Hello, world!");#printf("Hello, world!");include <stdio.h>
printf("Hello, world!");#incprintf("Hello, world!");lprintf("Hello, world!");uprintf("Hello, world!");dprintf("Hello, world!");eprintf("Hello, world!"); printf("Hello, world!");<printf("Hello, world!");math.h>
printf("Hello, world!");
printf("Hello, world!");iprintf("Hello, world!");nt main()
{
   // variables
   float a = 12.34 + 56.78;
   printf("%f\n", a);

   // infix operators
   printf("%f\n", 12.34 + 56.78);
   printf("%f\n", 12.34 - 56.78);
   printf("%f\n", 12.34 * 56.78);
   priprintf("Hello, world!");nprintf("Hello, world!");tf("%f\n", 12.34 / 56.78);

   // comparison operators
   printf("%d %d %d %d %dprintf("Hello, world!"); %d\n", 12.34 < 5printf("Hello, world!");6printf("Hello, world!");.78printf("Hello, world!");, 12.34 <= 56.78, 12.34 == 5printf("Hello, world!");6printf("Hello, world!");.78printf("Hello, world!");,printf("Hello, world!"); 12.34 >= 56.78, 12.34 > 56.78, 12.34 !printf("Hello, world!");= 56printf("Hello, world!");.78);
   printf("%d %d %d %dprintf("Hello, world!"); %d %d\n", 12.34 < 12.34, 12.34 <= 12.34, 12.34 == 12.34, 12.34 >= 12.34, 12.34 > 12.34, 12.34 != 12.34);
   printf("%d %d %d %d %d %d\n", 56.78 < 12.34, 56.78 <= 12.34, 56.78 == 12.34, 56.78 >= 12.34, 56.78 > 12.34, 56.78 != 12.34);

   // assignment operators
   a = 12.34;
   a += 56.78;
   printf("%f\n", a);

   a = 12.34;
   a -= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a *= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a /= 56.78;
   printf("%f\n", a);

   // prefix operators
   printf("%f\n", +12.34);
   printf("%f\n", -12.34);

   // type coercion
   a = 2;
   printf("%f\n", a);
   printf("%f\n", sin(2));

   return 0;
}

