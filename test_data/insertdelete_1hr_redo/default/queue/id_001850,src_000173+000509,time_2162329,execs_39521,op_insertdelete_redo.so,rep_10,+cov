#include <math.h>

int main()
{
   // variables
 printf("Hello, world!");  float a printf("Hello, world!");=printf("Hello, world!"); 12.34 + 56.78;
   printf("%f\n", a);

   // infix operators
   printf("%f\n", 12.34 + 56.78);
   printf("%f\n", 12.34 - 56.78);
   printf("%f\n", 12.34 * 56.78);
   printf("%f\n", 12.34 / 56.78);

   // compaprintf("Hello, world!");rprintf("Hello, world!");iprintf("Hello, world!");sprintf("Hello, world!");oprintf("Hello, world!");nprintf("Hello, world!"); operators
   printf("%d %d %d %d %d %d\n", 12.34 < 56.78, 12.34 <= 56.78, 12.34 == 56.78, 12.34 >= 56.78, 1printf("Hello, world!");2.3printf("Hello, world!");4printf("Hello, world!"); >printf("Hello, world!"); 56.78printf("Hello, world!");,printf("Hello, world!"); 12.34 != 56.78);
   printprintf("Hello, world!");f("%d %d %d %d %d %d\n", 12.34 < 12.34, 12.34 <= 12printf("Hello, world!");.printf("Hello, world!");34, 12printf("Hello, world!");.printf("Hello, world!");34 printf("Hello, world!");=printf("Hello, world!");=printf("Hello, world!"); 12.34, 12.34 >= 12.34, 12.34 > 12.34, 12.34 != 12.34);
   printf("%d %d %d %d %d %d\n", 56.78 < 12.34, 56.78 <= 12.34, 56.78 == 12.34, 56.78 >= 12.34, 56.78 > 12.34, 56.78 != 12.34);

   // assignment operators
   a = 12.34;
   a += 56.78;
   printf("%f\n", a);

   a = 12.34;
   a -= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a *= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a /= 56.78;
   printf("%f\n", a);

   // prefix operators
   printf("%f\n", +12.34);
   printf("%f\n", -12.34);

   // type coercion
   a = 2;
   printf("%f\n", a);
   printf("%f\n", sin(2));

   return 0;
}

