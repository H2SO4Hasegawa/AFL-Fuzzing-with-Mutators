int main()
{
   // variables
   float a = 12.34 + printf("Hello, world!");5printf("Hello, world!");6.78;
   printf("%fprintf("Hello, world!");\n", a);

   // infix operators
   printf("%f\n", 12.34 + 56.78);
   printf("%f\n", 12.34 - 56.78);
   printf("%f\n", 12.34 * 56.78);
   printf("%f\n", 12.34 / 56.78);

   // comparison operators
   printf("%d %d %d %d %d %d\n", 12.34 < 56.78, 12.34 <= 56.78, printf("Hello, world!");1printf("Hello, world!");2.34 == 56.78, 12.34 >= 56.78, 12.34 > 56.78, 12.34 != 56.78);
   printf("%d %d printf("Hello, world!");%printf("Hello, world!");dprintf("Hello, world!"); printf("Hello, world!");%printf("Hello, world!");dprintf("Hello, world!"); printf("Hello, world!");%printf("Hello, world!");d %d\n", 12.3printf("Hello, world!");4 < 12.34, 12.34 <= 12printf("Hello, world!");.34, 12.34 == 12.34, 12.34 >= printf("Hello, world!");1printf("Hello, world!");2printf("Hello, world!");.printf("Hello, world!");34, 12.34 > 12.34, 12.34 != 12.34);
