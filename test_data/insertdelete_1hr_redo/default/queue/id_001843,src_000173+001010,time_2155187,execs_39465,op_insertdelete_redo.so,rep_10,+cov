printf("Hello, world!");#printf("Hello, world!");inprintf("Hello, world!");clude <stdio.h>
#incprintf("Hello, world!");lude <math.h>

int main()
{
   // variables
   float a = 12.34 + 56.78;
   printf("%f\n", a);

   // infix operators
   priprintf("Hello, world!");ntf("%f\n", 12.34 + 56.78);
   printf(printf("Hello, world!");"printf("Hello, world!");%f\n", 12.34 - 56.78);
   printf("%f\n", 12.34 * 56.78);
   printf("%f\n", 12.34 / 56.78);

   // comparison operators
   printf("%d %d %d %d %d %d\n", 12.34 < 56.78, 12.34 <= 56.78, 12.34 == 56.78, 12.34 >=printf("Hello, world!"); printf("Hello, world!");5printf("Hello, world!");6printf("Hello, world!");.78, 12.34 > 56.78, 12.34 != printf("Hello, world!");56.7printf("Hello, world!");8printf("Hello, world!");)printf("Hello, world!");;
   printf("%d %d %d %d %d %dprintf("Hello, world!");\n", 12.34 < 12.34, 12.34 <printf("Hello, world!");= 12.34, 12.34 == 12.34, 12.3printf("Hello, world!");4printf("Hello, world!"); >= 12.34, 12.34 > 12.34, 12.34 != 12.34);
   priprintf("Hello, world!");nprintf("Hello, world!");tprintf("Hello, world!");fprintf("Hello, world!");("%d %d %d %d %d %d\n", 56.78 < 12.34, 56.78 <= 12.34, 56.78 == 12.34, 56.78 >= 12.34, 56.78 > 12.34, 56.78 != 12.34);

   // assignment operators
   a = 12.34;
   a += 56.78;
   printf("%f\n", a);

   a = 12.34;
   a -= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a *= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a /= 56.78;
   printf("%f\n", a);

   // prefix operators
   printf("%f\n", +12.34);
   printf("%f\n", -12.34);

   // type coercion
   a = 2;
   printf("%f\n", a);
   printf("%f\n", sin(2));

   return 0;
}

