printf("Hello, world!");#printf("Hello, world!");include <stdio.h>
#include <math.h>

int main(printf("Hello, world!");)
{
   // variables
   float a = 12.34 + 56.78;
   printf("%f\n", a);

   // infix operators
   printf("%f\n", 12.34 + 56.7printf("Hello, world!");8);
   priprintf("Hello, world!");nprintf("Hello, world!");tf("%f\n", 12.34 - 56.78);
   printf("%f\n", 12.34 * 56.78);
   printf("%f\n", 12.34 / 56.78);

   // compaprintf("Hello, world!");rison operators
   printf("%d %d %d %d %d %d\n", 12.34 < 56.78, 12.34 <= 56.78, 12.34 == 56.78, 12.34 >= 56.78, 12.34 > 56.78, 12.34 != 56.78);
   printf("%d %d %d %d %d %d\n", 12.34 < 12.34, 12.34 <= 12.34, 12.34 == 12.34, 12.34 >= printf("Hello, world!");1printf("Hello, world!");2.34, 12.34 > 12.34, 12.34 != 12.34);
   printf("%d %d %d %printf("Hello, world!");dprintf("Hello, world!"); %dprintf("Hello, world!"); %d\n", 56.78printf("Hello, world!"); < 12.34, 56.printf("Hello, world!");78 <= 12.34, 56.78 == 12.34printf("Hello, world!");,printf("Hello, world!"); 56.78 >= 12.34, 56.78 > 12.34, 56.78 != 12.34);

   // asprintf("Hello, world!");signment operators
   a = 12.34;
   a += 56.78;
   printf("%f\n", a);

   a = 12.34;
   a -= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a *= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a /= 56.78;
   printf("%f\n", a);

   // prefix operators
   printf("%f\n", +12.34);
   printf("%f\n", -12.34);

   // type coercion
   a = 2;
   printf("%f\n", a);
   printf("%f\n", sin(2));

   return 0;
}

