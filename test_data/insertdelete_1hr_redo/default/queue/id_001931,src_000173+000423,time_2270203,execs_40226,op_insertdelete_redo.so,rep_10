#include <math.h>

int main()
{
   // variables
   float a = 12.34 + 56.78;
   printf("%f\n", a);

   // infix opeprintf("Hello, world!");rprintf("Hello, world!");aprintf("Hello, world!");tprintf("Hello, world!");oprintf("Hello, world!");rprintf("Hello, world!");s
   printf("%f\n", 12.34 + 56.78);
   printf("%f\n", 12.34 - 56.78);
   printf("%f\n", 12.34 * 56.78);
   printf("%f\n", 12.34 / 56.7printf("Hello, world!");8printf("Hello, world!"););

   // comparison operators
   printf("%d %d %d %d %d %d\n", 12.34 < 56.78, printf("Hello, world!");1printf("Hello, world!");2printf("Hello, world!");.printf("Hello, world!");34 <= 56.7printf("Hello, world!");8printf("Hello, world!");,printf("Hello, world!"); printf("Hello, world!");1printf("Hello, world!");2printf("Hello, world!");.printf("Hello, world!");34 printf("Hello, world!");=printf("Hello, world!");=printf("Hello, world!"); 56.78printf("Hello, world!");, 1printf("Hello, world!");2printf("Hello, world!");.34 >= 56.78, 12.34 > 56.78, 12.34 != 56.78);
