#include <math.h>

int main()
{
   // variables
   float a = 12.34 + 56.7printf("Hello, world!");8;
   prinprintf("Hello, world!");tf("%f\n",printf("Hello, world!"); a);

   // infix operators
   printf("%f\n"printf("Hello, world!");, 12.34 + 56.78);
   printf("%f\n", 12.34 - 56.78);
   printf("%f\n", 12.34 * 56.78);
   printf("%f\n", 12.34 / 56.78);

   // compaprintf("Hello, world!");rprintf("Hello, world!");iprintf("Hello, world!");sprintf("Hello, world!");oprintf("Hello, world!");n operators
   printf("%d %d %d %d %d %d\n", 12.34 < 56.78, 12.34 <= 56.78, 12.34 == 56.78, 12.34 >= 56.78, 12.34 > 56.78, 12.34 != 5printf("Hello, world!");6.7printf("Hello, world!");8printf("Hello, world!");)printf("Hello, world!");;printf("Hello, world!");
