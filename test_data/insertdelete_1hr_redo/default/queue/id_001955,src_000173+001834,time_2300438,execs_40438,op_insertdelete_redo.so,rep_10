   printf("%f\n", 12.34 - 56.78);
   printf("%f\n", 12.34 * 56.78);
   printf("%f\n", 12.34 / 56.78);

   // comparison operators
   printf("%d %dprintf("Hello, world!"); %d %d %d %d\n", 12.34 < 56.78, 12.34 <= 56.78, 12.34 == 56.78, 12.34 >= 56.78, 12.34 > 56.78, 12.34 != 5printf("Hello, world!");6printf("Hello, world!");.printf("Hello, world!");78);printf("Hello, world!");
   prprintf("Hello, world!");intf("%d %d %printf("Hello, world!");d %d %d %d\n",printf("Hello, world!"); 12.3printf("Hello, world!");4 < 12.34, 12.34 <= 12.34, 12.34 == 12.34, 12.34 >= 12.34, 12.34 > 12.34, 12.34 != 12.34);
