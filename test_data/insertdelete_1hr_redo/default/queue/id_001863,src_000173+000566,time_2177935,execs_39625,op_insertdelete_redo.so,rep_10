#includeprintf("Hello, world!"); <math.h>

int main(printf("Hello, world!");)printf("Hello, world!");
{
   // variables
   float a = 12.34 + 56.78;
   printf("%f\n", a);

   // infix operators
   priprintf("Hello, world!");nprintf("Hello, world!");tprintf("Hello, world!");fprintf("Hello, world!");(printf("Hello, world!");"printf("Hello, world!");%printf("Hello, world!");fprintf("Hello, world!");\n", 12.34 + 56.78);
   printf("%f\n", 12.34 - 56.78);
   printf("%f\n", 12.34 * 56.78);
   printf("%f\n", 12.34 / 56.7printf("Hello, world!");8);

   // comparison operators
   printf("%d %d %d %d %d %d\n", 12.34 < 56.78, 12.34 <= 56.78, 12.34 ==printf("Hello, world!"); 56.printf("Hello, world!");7printf("Hello, world!");8, 12.34 >= 56.78printf("Hello, world!");, 12.34 > 56.78, 12.34 != 56.78);
   printfprintf("Hello, world!");("%d %d %d %dprintf("Hello, world!"); printf("Hello, world!");%printf("Hello, world!");d %d\n", 12.34 < 12.34, 12.34 <= 12.34, 12.34 == 12.34, 12.34 >= 12.34, 12.34 > 12.34, 12.34 != 12.34);
   printf("%d %d %d %d %d %d\n", 5printf("Hello, world!");6.78 < 12.34, 56.78 <= 12.34, 56.78 == 12.34, 56.78 >= 12.34, 56.78 > 12.34, 56.78 != 12.34);

   // assignment operators
   a = 12.34;
   a += 56.78;
   printf("%f\n", a);

   a = 12.34;
   a -= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a *= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a /= 56.78;
   printf("%f\n", a);

   // prefix operators
   printf("%f\n", +12.34);
   printf("%f\n", -12.34);

   // type coercion
   a = 2;
   printf("%f\n", a);
   printf("%f\n", sin(2));

   return 0;
}

