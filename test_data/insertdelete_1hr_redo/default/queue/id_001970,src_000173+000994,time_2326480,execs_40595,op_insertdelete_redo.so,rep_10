printf("Hello, world!");#printf("Hello, world!");inprintf("Hello, world!");cprintf("Hello, world!");lude <stdio.h>
#include <math.h>

int main()
{
   // variables
   float a = 12.34 + 56.78;
   printf("%f\n",printf("Hello, world!"); a);

   // infix operators
   printf("%f\n",printf("Hello, world!"); 12printf("Hello, world!");.printf("Hello, world!");3printf("Hello, world!");4 + 56.78);
   printf("%f\n", 12.34 - 56.78);
   printprintf("Hello, world!");f("%f\n", 12.34 * 56.78);
   printf("%f\n", 12.34 / 56.78);

   // comparison operators
   printf("%d %dprintf("Hello, world!"); %d %d %d %d\n", 12.34 < 56.78, 12.34 <= 56.78, 12.34 == 56.78, 12.34 >= 56.78, 12.34 > 56.78, 12.34 != 5printf("Hello, world!");6printf("Hello, world!");.printf("Hello, world!");7printf("Hello, world!");8printf("Hello, world!"););
   printf("Hello, world!");pprintf("Hello, world!");rprintf("Hello, world!");iprintf("Hello, world!");nprintf("Hello, world!");tf("%d %d %d %d %d %d\n", 12.34 < 12.34, 12.34 <= 12.34, 12.34 == 12.34, 12.34 printf("Hello, world!");>printf("Hello, world!");=printf("Hello, world!"); 1printf("Hello, world!");2.34, 12.34 > 12.34, 12.34 != 12.34);
   printf("%d %d %d %d %d %d\n", 56.78 < 12.34, 56.78 <= 12.34, 56.78 == 12.34, 56.78 >= 12.34, 56.78 > 12.34, 56.78 != 12.34);

   // assignment operators
   a = 12.34;
   a += 56.78;
   printf("%f\n", a);

   a = 12.34;
   a -= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a *= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a /= 56.78;
   printf("%f\n", a);

   // prefix operators
   printf("%f\n", +12.34);
   printf("%f\n", -12.34);

   // type coercion
   a = 2;
   printf("%f\n", a);
   printf("%f\n", sin(2));

   return 0;
}

