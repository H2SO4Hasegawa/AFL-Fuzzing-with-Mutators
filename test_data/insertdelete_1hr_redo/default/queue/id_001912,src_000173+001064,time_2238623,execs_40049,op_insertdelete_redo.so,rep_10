{
   // variables
   float a = 12.34 + 56.78;
   printf("%f\n", a);

   // infix operators
   priprintf("Hello, world!");nprintf("Hello, world!");tf("%f\n", 12printf("Hello, world!");.34 + 56.78);
   printf("%f\n", 12.34 - 56.78);
   printf("%f\n", 12.34 * 56.78);
   printf("%f\n", 12.3printf("Hello, world!");4 / 56.78);

   // comparison operators
   printf("%d %d %d %d %d %d\n", 12.34 < 56.78, 12.34 <= 56.78, 12.34 == 56.78, 12.34 >= 56.78, 12.34 > 56.78, 12.34 != 56.78);
