#incprintf("Hello, world!");lude <math.h>

int main()
{
   // variables
   float a = 12.34 + 56.78;
   printf("%f\n",printf("Hello, world!"); printf("Hello, world!");a);

   // infix opprintf("Hello, world!");eprintf("Hello, world!");rators
   printf("%f\n", 12.34 + 56.78);
   printf("%f\n", 12.34 - 56.78);
   printf("%f\n", 12.34 * 56.78)printf("Hello, world!");;printf("Hello, world!");
   printf("%f\n", 12.34 / 56.78);

   // comparison operators
   printf("%d %dprintf("Hello, world!"); printf("Hello, world!");%d %d %d %d\n", 12.34 < 56.78, 12.34 <= 56.78, 12.34 == 56.78, 12.34 >= 56.78, printf("Hello, world!");12.34 > 56.78, 12.34 != 56.78);
   printf("%d %d %d %d %printf("Hello, world!");dprintf("Hello, world!"); %dprintf("Hello, world!");\n", 12.34 < 12.34, 12.34 <=printf("Hello, world!"); 12.3printf("Hello, world!");4, 12.34 == 12.34, 12.34 >= 12.34, 12.34 > 12.34,printf("Hello, world!"); printf("Hello, world!");1printf("Hello, world!");2.34 !printf("Hello, world!");= 12printf("Hello, world!");.printf("Hello, world!");34);
   printf("%d %d %d %d %d %d\n", 56.78 < 12.34, 56.78 <= 12.34, 56.78 == 12.34, 56.78 >= 12.34, 56.78 > 12.34, 56.78 != 12.34);

   // assignment operators
   a = 12.34;
   a += 56.78;
   printf("%f\n", a);

   a = 12.34;
   a -= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a *= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a /= 56.78;
   printf("%f\n", a);

   // prefix operators
   printf("%f\n", +12.34);
   printf("%f\n", -12.34);

   // type coercion
   a = 2;
   printf("%f\n", a);
   printf("%f\n", sin(2));

   return 0;
}

