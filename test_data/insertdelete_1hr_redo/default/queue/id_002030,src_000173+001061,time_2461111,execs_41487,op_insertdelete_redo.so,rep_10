   printf("%f\n", 12.34 - 56.78);
   printf("%f\n", 12.34 * 56.78);
   printf("%f\n", 12.3printf("Hello, world!");4printf("Hello, world!"); printf("Hello, world!");/printf("Hello, world!"); 56.78);

   // comparison opprintf("Hello, world!");eprintf("Hello, world!");rprintf("Hello, world!");ators
   printf("%d %d %d %d %d %d\n", 12.34 < 56.78, 1printf("Hello, world!");2.34 <= 56.78, 12.34 == 56.78, 1printf("Hello, world!");2.34 >= 56.78, 12.34 > 56.78printf("Hello, world!");,printf("Hello, world!"); 12printf("Hello, world!");.34 != 56.78)printf("Hello, world!");;printf("Hello, world!");
