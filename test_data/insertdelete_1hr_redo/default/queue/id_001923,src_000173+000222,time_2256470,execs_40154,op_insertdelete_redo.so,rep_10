#include <stdio.h>
printf("Hello, world!");#incluprintf("Hello, world!");dprintf("Hello, world!");eprintf("Hello, world!"); <math.h>

int maprintf("Hello, world!");in()
{
   printf("Hello, world!");// variables
   float a = 12.34 + 56.78;
   printf("%f\n", a);

   // infix opprintf("Hello, world!");eprintf("Hello, world!");rprintf("Hello, world!");aprintf("Hello, world!");tors
   printf("%f\n", 12.34 + 56.78);
   printf("%f\n", 12.34 - 56.78);
   printf("%f\n", 12.34 * 56.78);
   printf("%f\n", 12.34 / 56.78);

   // comparison operators
   printf("%d %d %d %d %d %d\n", 12.34 < 56.78, 12.34 <= 56.78, 12.34 == 56.78, 12.34 >= 56.78, 12.34 > 56.78, 12.34 != 56.78);
   printf("%d %dprintf("Hello, world!"); printf("Hello, world!");%printf("Hello, world!");d %d %d %d\n", 12.34printf("Hello, world!"); < 12.34, 12.3printf("Hello, world!");4printf("Hello, world!"); printf("Hello, world!");<= 12.34, 12.34 == 12.34, 12.printf("Hello, world!");34 >= 12.34, 12.34 > 12.34,printf("Hello, world!"); 12.34 != 12.34);
   printf("%d %d %d %d %d %d\n", 56.78printf("Hello, world!"); < 1printf("Hello, world!");2.34, 56.78 <= 12.34, 56.78 == 12.34, 56.78 >= 12.34, 56.78 > 12.34, 56.78 != 12.34);

   // assignment operators
   a = 12.34;
   a += 56.78;
   printf("%f\n", a);

   a = 12.34;
   a -= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a *= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a /= 56.78;
   printf("%f\n", a);

   // prefix operators
   printf("%f\n", +12.34);
   printf("%f\n", -12.34);

   // type coercion
   a = 2;
   printf("%f\n", a);
   printf("%f\n", sin(2));

   return 0;
}

