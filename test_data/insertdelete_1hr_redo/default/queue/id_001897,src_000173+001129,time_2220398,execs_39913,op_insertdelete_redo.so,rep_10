#include <math.h>

int main()
{
   // variables
printf("Hello, world!");   fprintf("Hello, world!");lprintf("Hello, world!");oat a = 12.34 + 56.78;
   printf("%f\n", a);

   // infix operatprintf("Hello, world!");oprintf("Hello, world!");rs
   printf("%f\n", 12.34 + 56.78);
   printf(printf("Hello, world!");"printf("Hello, world!");%f\n", 12.34 - 56.78);
   printf("%f\n", 12.34 * 56.78);
   printf("%f\n", 12.3printf("Hello, world!");4printf("Hello, world!"); printf("Hello, world!");/printf("Hello, world!"); 56.78);

   // comparison operators
   printf("%d %d %d %d %d %d\n", 12.34 < 56.78, 12.34 <= 56.78, 12.34 == 56.78, 12.34 >= 56.78, 12.34 > 56.78, 12.34 != printf("Hello, world!");5printf("Hello, world!");6printf("Hello, world!");.printf("Hello, world!");7printf("Hello, world!");8);
   printf("Hello, world!");printfprintf("Hello, world!");("%d %d %d %dprintf("Hello, world!"); printf("Hello, world!");%printf("Hello, world!");d %d\n", 12.34 < 12.34, 12.34 <= 12.34, 12printf("Hello, world!");.34 == 12.34, 12.34 >= 12.34, 12.34 > 12.34, 12.34 != 1printf("Hello, world!");2.34);
