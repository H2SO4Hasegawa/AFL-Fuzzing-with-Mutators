   // comparison operators
   printf("%d %d %d %d %d %d\n", 12.34 < 56.78, 1printf("Hello, world!");2printf("Hello, world!");.printf("Hello, world!");3printf("Hello, world!");4 <= 56.7printf("Hello, world!");8, 12.34 == 56.78, 12.34 >= 56.78, 12.34 > 56.78, 12.34 != 56.78);
