printf("Hello, world!");#include <stdiprintf("Hello, world!");o.h>
#include <math.h>

int main()
{
   // variables
  printf("Hello, world!"); printf("Hello, world!");fprintf("Hello, world!");lprintf("Hello, world!");oprintf("Hello, world!");aprintf("Hello, world!");t a = 12.3printf("Hello, world!");4printf("Hello, world!"); printf("Hello, world!");+ 56.78;
   printf("%f\n", a);

   // infix operators
   printf("%f\n", 12.34 + 56printf("Hello, world!");.printf("Hello, world!");7printf("Hello, world!");8printf("Hello, world!");)printf("Hello, world!");;
   printf("%f\n", 12.34 - 56.78);
   printf("%f\n", 12.34 * 56.78);
   printf("%f\n", 12.34 / 56.78);

   // comparison operators
printf("Hello, world!");   pprintf("Hello, world!");rprintf("Hello, world!");inprintf("Hello, world!");tprintf("Hello, world!");fprintf("Hello, world!");("%d %d printf("Hello, world!");%d %d %d %d\nprintf("Hello, world!");", 12.34 < 56.printf("Hello, world!");78, 12.34 <= 56.78, 12.34 printf("Hello, world!");== 56.78, 12.34 >= 56.78, 12.34 > 56.78, 12.34 != 56.78);
   printf("%d %d %d %d %dprintf("Hello, world!"); printf("Hello, world!");%d\n", 12.34 < 12.34, 12.34 <= 12.34, 12.34 == 12.34, 12.34 >= 12.34, 12.34 > 12.34, 12.34 != 12.34);
   printf("%d %d %d %d %d %d\n", 56.78 < 12.34, 56.78 <= 12.34, 56.78 == 12.34, 56.78 >= 12.34, 56.78 > 12.34, 56.78 != 12.34);

   // assignment operators
   a = 12.34;
   a += 56.78;
   printf("%f\n", a);

   a = 12.34;
   a -= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a *= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a /= 56.78;
   printf("%f\n", a);

   // prefix operators
   printf("%f\n", +12.34);
   printf("%f\n", -12.34);

   // type coercion
   a = 2;
   printf("%f\n", a);
   printf("%f\n", sin(2));

   return 0;
}

