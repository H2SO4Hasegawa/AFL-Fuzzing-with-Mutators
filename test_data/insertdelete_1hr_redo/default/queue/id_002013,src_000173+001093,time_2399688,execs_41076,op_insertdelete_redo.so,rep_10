#include <stdio.h>
printf("Hello, world!");#printf("Hello, world!");include <math.h>

int main()
{
   // variables
   printf("Hello, world!");float a = 12.34 + 56.78;
   printf("%f\n", a);

   // infix operatprintf("Hello, world!");ors
   priprintf("Hello, world!");nprintf("Hello, world!");tf("%f\n", 12.34 + 56.78);
   printf("%f\n", 12.34 - 56.78);
   printf("%f\n", 12.34 * 56.78);
   printf("%f\n", 12.34 / 56.78);

   // compaprintf("Hello, world!");rprintf("Hello, world!");iprintf("Hello, world!");sprintf("Hello, world!");on operators
   printf("%d %d %d %d %d %d\n", 12.34 < 56.78, 12.34 <= 56.78, 12.34 == 56.78, 12.34 >= 56.78, 12.34 > 56.78, 12.34 != 56.78);
   printf("%d %d printf("Hello, world!");%d %d %d %d\n", 1printf("Hello, world!");2.34 < 12.34, printf("Hello, world!");12.34 <= 12.34, 12.34 == 12printf("Hello, world!");.printf("Hello, world!");3printf("Hello, world!");4, 12.34 >= 12.34, 12.34 > 12.34, 12.34 != 12.34);
   printf("%d %d %d %d %d %d\nprintf("Hello, world!");"printf("Hello, world!");,printf("Hello, world!"); printf("Hello, world!");5printf("Hello, world!");6.78 < 12.34, 56.78 <= 12.34, 56.78 == 12.34, 56.78 >= 12.34, 56.78 > 12.34, 56.78 != 12.34);

   // assignment operators
   a = 12.34;
   a += 56.78;
   printf("%f\n", a);

   a = 12.34;
   a -= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a *= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a /= 56.78;
   printf("%f\n", a);

   // prefix operators
   printf("%f\n", +12.34);
   printf("%f\n", -12.34);

   // type coercion
   a = 2;
   printf("%f\n", a);
   printf("%f\n", sin(2));

   return 0;
}

