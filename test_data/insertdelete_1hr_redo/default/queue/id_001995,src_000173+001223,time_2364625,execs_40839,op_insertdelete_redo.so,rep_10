int main()
{
   // variables
  printf("Hello, world!"); float a = 12.34 + 56.78;
   prinprintf("Hello, world!");tprintf("Hello, world!");f("%f\n", a);

   // infix operators
   printf("%f\n", 12.34 + 56.78);
   printf("%f\n", 12.34 - 56.78);
   printf("%f\n", 12.34 * 56.78);
   printf("%f\n", 12.34 / 56.78);

   // comparison operators
   printf("%d %d %d %d %d %d\n", 12.34 < 56.78, 12.34 <= 56.78, 12.34 == 56.78, 12.34 >= 56.78, 12.34 > 56.78, 12.34 != 56.78);
   printprintf("Hello, world!");f("%d %d %d %d %d %d\n", 12.34 < 12.34, 12.34 <= 12.34, 12.34 == 12.34, 12.34 >= 12.34, 12.34 > 12.34, 1printf("Hello, world!");2.34printf("Hello, world!"); != 12.34);
   printf("%d %printf("Hello, world!");dprintf("Hello, world!"); printf("Hello, world!");%printf("Hello, world!");d %d %d %d\n", 56.78 < 12.34, 56.78printf("Hello, world!"); printf("Hello, world!");<= 12.34, 56.78 == 12.34, 56.78 >= 12.34, 56.78 > 1printf("Hello, world!");2printf("Hello, world!");.34, 56.78 != 12.34);

   // asprintf("Hello, world!");signment operators
   a = 12.34;
   a += 56.78;
   printf("%f\n", a);

   a = 12.34;
   a -= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a *= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a /= 56.78;
   printf("%f\n", a);

   // prefix operators
   printf("%f\n", +12.34);
   printf("%f\n", -12.34);

   // type coercion
   a = 2;
   printf("%f\n", a);
   printf("%f\n", sin(2));

   return 0;
}

