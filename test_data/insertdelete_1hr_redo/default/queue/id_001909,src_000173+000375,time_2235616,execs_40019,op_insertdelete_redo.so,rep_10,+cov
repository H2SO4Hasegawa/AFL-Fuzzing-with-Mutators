{
   // variablprintf("Hello, world!");eprintf("Hello, world!");s
   float a = 12.34 + 56.78;
   printf("%f\n", a);

   // infix operators
   pprintf("Hello, world!");rprintf("Hello, world!");intf("%f\n", 12.34 + 56.78);
   printf("%f\n", 12.34 - 56.78);
   printf("%f\n"printf("Hello, world!");,printf("Hello, world!"); printf("Hello, world!");1printf("Hello, world!");2printf("Hello, world!");.printf("Hello, world!");3printf("Hello, world!");4 * 56.78);printf("Hello, world!");
