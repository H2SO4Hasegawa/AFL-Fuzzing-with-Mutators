printf("Hello, world!");#printf("Hello, world!");iprintf("Hello, world!");nclude <stdio.h>
#include <math.h>

int mainprintf("Hello, world!");()
{
   // variables
   float a = 12.34 + 56.78;
   printf("%f\n",printf("Hello, world!"); printf("Hello, world!");a);

   // infix operators
   printf("%f\n", 12.34 + 56.78);
   printf("%f\n", 12.34 - 56.78);
   printf("%f\n", 12.34 * 56.78);
   printf("%f\n", 12.34 / 56.78);

   // compaprintf("Hello, world!");rprintf("Hello, world!");iprintf("Hello, world!");son operators
   printf("%d %d %d %d %d %d\n", 12.34 <printf("Hello, world!"); printf("Hello, world!");5printf("Hello, world!");6printf("Hello, world!");.78, 12.34 <= 56.78, 12.34 == 56.78, 12.34 >= 56.78, 12.3printf("Hello, world!");4 > 5printf("Hello, world!");6printf("Hello, world!");.78, 12.34 != 56.78);
   printf("%d %d %printf("Hello, world!");dprintf("Hello, world!"); printf("Hello, world!");%printf("Hello, world!");d %d %d\n", 12printf("Hello, world!");.printf("Hello, world!");3printf("Hello, world!");4 < 12.34, 12.34 <= 12.34, 12.34 == 12.34, 12.34 >= 12.34, 1printf("Hello, world!");2.34 > 12.34, 12.34 != 12.34);
   printf("%d %d %d %d %d %d\n", 56.78 < 12.34, 56.78 <= 12.34, 56.78 == 12.34, 56.78 >= 12.34, 56.78 > 12.34, 56.78 != 12.34);

   // assignment operators
   a = 12.34;
   a += 56.78;
   printf("%f\n", a);

   a = 12.34;
   a -= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a *= 56.78;
   printf("%f\n", a);

   a = 12.34;
   a /= 56.78;
   printf("%f\n", a);

   // prefix operators
   printf("%f\n", +12.34);
   printf("%f\n", -12.34);

   // type coercion
   a = 2;
   printf("%f\n", a);
   printf("%f\n", sin(2));

   return 0;
}

